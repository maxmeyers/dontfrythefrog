#import "BTAlertView.h"
#import "BTIncludes.h"

@implementation BTAlertView

- (id)init
{
    self = [super init];
    if (self) {
        BTPlayer *player = [[[(AppDelegate *)[[UIApplication sharedApplication] delegate] gameScene] gameLayer] player];
        player.touch0 = nil;
        player.touch1 = nil;
    }
    
    return self;
}

@end
